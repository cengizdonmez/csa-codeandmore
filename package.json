{
  "name": "ant-design-pro",
  "version": "5.0.0-alpha.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=en-US --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev --openssl-legacy-provider start",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^6.3.0",
    "@ant-design/pro-provider": "^2.0.12",
    "@ant-design/pro-table": "^3.1.0",
    "@ant-design/pro-utils": "0.0.5",
    "@tinymce/tinymce-react": "^4.2.0",
    "@types/array-move": "^2.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/react-color": "^3.0.4",
    "@types/react-sortable-hoc": "^0.7.1",
    "@types/react-sortable-tree": "^0.3.12",
    "@types/uuid": "^8.3.0",
    "@umijs/plugins": "^4.0.64",
    "antd": "^4.7.0",
    "antd-image-cropper": "^0.4.0",
    "antd-img-cropper": "^1.0.0",
    "antd-mask-input": "^2.0.7",
    "apexcharts": "^3.26.1",
    "array-move": "^3.0.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "country-list-js": "^3.1.7",
    "depcheck": "^1.4.3",
    "hox": "^1.1.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "omit.js": "^2.0.2",
    "prismjs": "^1.23.0",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-apexcharts": "^1.3.9",
    "react-color": "^2.18.1",
    "react-countup": "^6.4.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.8.6",
    "react-image-crop": "^10.0.6",
    "react-nestable": "^1.3.0",
    "react-password-strength": "^2.4.0",
    "react-quill": "^1.3.5",
    "react-simple-code-editor": "^0.11.0",
    "react-sortable-hoc": "^1.11.0",
    "react-sortable-tree": "^2.8.0",
    "umi": "^3.2.0",
    "umi-request": "^1.0.8",
    "umi-router": "^0.1.2",
    "use-merge-value": "^1.0.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.144",
    "@types/prismjs": "^1.16.2",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@umijs/fabric": "^2.2.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.0-beta.2",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/preset-ui": "^2.0.9",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "puppeteer-core": "^5.0.0",
    "stylelint": "^13.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
